            <div class="Heading" id="Stats-Heading">Player Stats
                <img src="/resources/icons/bats.png" alt="">
            </div>



            <div class="wallet" id="wallet"></div>





            <div class="bids">


                <div id="search">

                    <input id="searchOrder" name="order" size="3" type="text" value="">
                    <button class="button" id="change">Search</button>

                </div>

                <section class="bids-1">
                    <div class="bids-3" id="bids-3">
                        <p class="Bid">Current Bid:</p>
                        <p id="rupee"> &#8377<span class="Bid" id="Amount">0</span>Lakhs</p>
                    </div>
                    <div class="upsold">
                        <img id="PREV" src="/resources/icons/next.png" alt="">
                        <button class="button" id="UP">Bid Up</button>
                        <button class="button" id="Sold">Sell</button>      
                        <img ID="NEXT" src="/resources/icons/next.png" alt="">
                    </div>
                    <div>
                        <p class="Bids" id="hidden"></p>
                    </div>

                </section>


                <section class="undo">
                    <button class="button" id="undo">Undo</button>
                    <button class="button" id="reset">Reset</button>
                </section>


                <section class="teamSelector" id="teamSelector">
                    <p class="edgy-font">Select Team</p>

                    <form>
                        <div class="buying-teams" id="buying-teams">
                            <label id="mi"><input name="profession" type="radio" value="MI"><img class="logo"
                                    src="/resources/logos/milogo.webp" alt=""></label>
                            <label id="csk"><input name="profession" type="radio" value="CSK"> <img class="logo"
                                    src="/resources/logos/csklogo.webp" alt=""></label>
                            <label id="rr"><input name="profession" type="radio" value="RR"> <img class="logo"
                                    src="/resources/logos/rrlogo.webp" alt=""></label>
                            <label id="srh"><input name="profession" type="radio" value="SRH"> <img class="logo"
                                    src="/resources/logos/srhlogo.webp" alt=""></label>

                            <label id="rcb"><input name="profession" type="radio" value="RCB"> <img class="logo"
                                    src="/resources/logos/rcblogo.webp" alt=""></label>
                            <label id="pbks"><input name="profession" type="radio" value="PBKS"> <img class="logo"
                                    src="/resources/logos/pbkslogo.webp" alt=""></label>
                            <label id="kkr"><input name="profession" type="radio" value="KKR"> <img class="logo"
                                    src="/resources/logos/kkrlogo.webp" alt=""></label>
                            <label id="dc"><input name="profession" type="radio" value="DC"> <img class="logo"
                                    src="/resources/logos/dclogo.webp" alt=""></label>
                        </div>
                    </form>
                    <div class="add">
                        <button class="button" id="confirm">ADD</button>
                    </div>
                </section>

            </div>

            <div class="statsHolder">
                <div class="statsHolderBackground" id="statsHolderBackground">
                    <p id="playerno">Player:<span id="playerNum"></span></p>
                    <p class="stats" id="name"></p>


                    <img class="stas" id="currPlayerImg" src="" alt="">

                    <div class="data" id="databox">
                        <p>Bat Pts :<span id="batAvg"></span></p>

                        <p>Bowl Pts:<span id="bowlPts"></span></p>

                        <p>Wk Pts :<span id="wkPts"></span></p>



                    </div>
                </div>

                <div class="otherstatsbg">
                    <div class="team-role">


                        <p id="role">-</p>
                        <img class="eclipse" src="resources/icons/eclipse.webp" alt="">
                        <img id="roleImg" src="" alt="">
                    </div>

                    <div class="nationality">
                        <p id="nationality">-</p>
                        <img class="eclipse" src="resources/icons/eclipse.webp" alt="">
                        <img class="flag" id="flagImg" src="" alt="">
                    </div>
                    <div class="basePrice">
                        <p>Base- <span id="basePrice">-</span> Cr</p>
                        <img class="eclipse" src="resources/icons/eclipse.webp" alt="">
                        <img class="coin" src="resources/icons/BasePrice.webp" alt="">
                    </div>
                </div>

            </div>

        </section>

